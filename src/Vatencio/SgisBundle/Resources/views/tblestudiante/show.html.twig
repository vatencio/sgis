{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tblestudiante</h1>

    <table>
        <tbody>
            <tr>
                <th>Intidestudiante</th>
                <td>{{ tblestudiante.intidestudiante }}</td>
            </tr>
            <tr>
                <th>Dtmcreacion</th>
                <td>{% if tblestudiante.dtmcreacion %}{{ tblestudiante.dtmcreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Strcreacion</th>
                <td>{{ tblestudiante.strcreacion }}</td>
            </tr>
            <tr>
                <th>Dtmactualizacion</th>
                <td>{% if tblestudiante.dtmactualizacion %}{{ tblestudiante.dtmactualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Stractualizacion</th>
                <td>{{ tblestudiante.stractualizacion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tblestudiante_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tblestudiante_edit', { 'id': tblestudiante.intidestudiante }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
