{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tblperiodos list</h1>

    <table>
        <thead>
            <tr>
                <th>Intidperiodo</th>
                <th>Strnombre</th>
                <th>Dtmcreacion</th>
                <th>Strcreacion</th>
                <th>Dtmactualizacion</th>
                <th>Stractualizacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tblperiodo in tblperiodos %}
            <tr>
                <td><a href="{{ path('tblperiodo_show', { 'id': tblperiodo.intidperiodo }) }}">{{ tblperiodo.intidperiodo }}</a></td>
                <td>{{ tblperiodo.strnombre }}</td>
                <td>{% if tblperiodo.dtmcreacion %}{{ tblperiodo.dtmcreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ tblperiodo.strcreacion }}</td>
                <td>{% if tblperiodo.dtmactualizacion %}{{ tblperiodo.dtmactualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ tblperiodo.stractualizacion }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('tblperiodo_show', { 'id': tblperiodo.intidperiodo }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('tblperiodo_edit', { 'id': tblperiodo.intidperiodo }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tblperiodo_new') }}">Create a new tblperiodo</a>
        </li>
    </ul>
{% endblock %}
