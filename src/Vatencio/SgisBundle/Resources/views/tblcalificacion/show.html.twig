{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tblcalificacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Intidcalificacion</th>
                <td>{{ tblcalificacion.intidcalificacion }}</td>
            </tr>
            <tr>
                <th>Strnombre</th>
                <td>{{ tblcalificacion.strnombre }}</td>
            </tr>
            <tr>
                <th>Strdescripcion</th>
                <td>{{ tblcalificacion.strdescripcion }}</td>
            </tr>
            <tr>
                <th>Dtmcreacion</th>
                <td>{% if tblcalificacion.dtmcreacion %}{{ tblcalificacion.dtmcreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Strcreacion</th>
                <td>{{ tblcalificacion.strcreacion }}</td>
            </tr>
            <tr>
                <th>Dtmactualizacion</th>
                <td>{% if tblcalificacion.dtmactualizacion %}{{ tblcalificacion.dtmactualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Stractualizacion</th>
                <td>{{ tblcalificacion.stractualizacion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tblcalificacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tblcalificacion_edit', { 'id': tblcalificacion.intidcalificacion }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
